---
interface Props {
	title: string;
}
const { title } = Astro.props;
---
<div>
  <br class="mof--hidden">
  <details class="rf--hidden">
    <summary>
      <div><h2 class="section nopadding">{title}</h2></div></summary>
     <slot />
  </details>
  <div class="mof--hidden">
    <h2>{title}</h2>
     <slot />
  </div>
</div>